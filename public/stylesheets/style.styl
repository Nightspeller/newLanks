body
  background url("../images/template/bg.jpg") repeat-x #0E3B72
  font-family 'Open Sans', sans-serif
  font-size 12pt
  color #222222
  img
    border-style none
  p
    text-align justify
    text-indent 2pc
    padding-right 20px
  h2, h3
    text-align center
  #wrapper
    margin auto
    padding 10px
    padding-bottom 1px
    width 1200px
    border-radius 15px
    box-shadow 5px 5px 15px #000
    background #ebf1f6
    background -moz-linear-gradient(top,  #ebf1f6 0%, #b3fcf3 67%, #89c3eb 100%, #d5ebfb 100%)
    background -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ebf1f6), color-stop(67%,#b3fcf3), color-stop(100%,#89c3eb), color-stop(100%,#d5ebfb))
    background -webkit-linear-gradient(top,  #ebf1f6 0%,#b3fcf3 67%,#89c3eb 100%,#d5ebfb 100%)
    background -o-linear-gradient(top,  #ebf1f6 0%,#b3fcf3 67%,#89c3eb 100%,#d5ebfb 100%)
    background -ms-linear-gradient(top,  #ebf1f6 0%,#b3fcf3 67%,#89c3eb 100%,#d5ebfb 100%)
    background linear-gradient(to bottom,  #ebf1f6 0%,#b3fcf3 67%,#89c3eb 100%,#d5ebfb 100%)
    #snowSwitch
      position fixed
      top 10px
      right 10px
    #header
      background-color #f0f8ff
      border #6495ed 1px solid
      border-radius 15px
      padding 10px 0
      table
        width 100%
      td
        width 25%
        text-align center
      #logo_img
        margin auto
        width 80%
      #header_img
        margin auto
        width 80%
        border-radius 40px
      address
        text-align center
        color #0255D4
        font-size 1.2em
        font-style normal
        strong.green
          color green
          a
            color green
  #footer
    text-align center
    margin 10px
    color #ffffff
#menu
  height 135px
  background-image url("../images/template/bg_menu.png")
  background-repeat repeat-x
  margin 10px -10px
  border-color #6495ed
  border-width 1px
  border-style solid
  border-radius 0
  text-align center
  table
    width 100%
  td
    padding 0.85em
.button_1
  position relative
  display block
  margin auto
  width 210px
  background-color rgb(71, 127, 178)
  border-style solid
  border-width 1px
  border-radius 15px
  border-color #186f8f
  padding 8px
  color #ffffff
  text-decoration none
  font-size 1em
  z-index 1
  box-shadow inset 0px 1px 0px #c4d0f9, 0px 2px 0px #AAAAAA
  background-image linear-gradient(bottom, rgb(71, 127, 178) 0%, rgb(124, 180, 218) 100%)
  background-image -o-linear-gradient(bottom, rgb(71, 127, 178) 0%, rgb(124, 180, 218) 100%)
  background-image -moz-linear-gradient(bottom, rgb(71, 127, 178) 0%, rgb(124, 180, 218) 100%)
  background-image -webkit-linear-gradient(bottom, rgb(71, 127, 178) 0%, rgb(124, 180, 218) 100%)
  background-image -ms-linear-gradient(bottom, rgb(71, 127, 178) 0%, rgb(124, 180, 218) 100%)
  background-image -webkit-gradient(linear,left bottom,left top,color-stop(0, rgb(71, 127, 178)),color-stop(1, rgb(124, 180, 218)))
  &:active
    padding-bottom 7px
    padding-left 8px
    padding-right 8px
    padding-top 9px
    top 1px
    background-image linear-gradient(bottom, rgb(124, 180, 218) 0%, rgb(71, 127, 178) 100%)
    background-image -o-linear-gradient(bottom, rgb(124, 180, 218) 0%, rgb(71, 127, 178) 100%)
    background-image -moz-linear-gradient(bottom, rgb(124, 180, 218) 0%, rgb(71, 127, 178) 100%)
    background-image -webkit-linear-gradient(bottom, rgb(124, 180, 218) 0%, rgb(71, 127, 178) 100%)
    background-image -ms-linear-gradient(bottom, rgb(124, 180, 218) 0%, rgb(71, 127, 178) 100%)
    background-image -webkit-gradient(
    linear,
    left bottom,
    left top,
    color-stop(0, rgb(124, 180, 218)),
    color-stop(1, rgb(71, 127, 178))
    )
  &:hover
    text-decoration underline
.button_2
  position relative
  display block
  margin auto
  width 160px
  background-color #3bb3e0
  border-style solid
  border-width 1px
  border-radius 15px
  border-color #186f8f
  padding 8px
  color #ffffff
  text-decoration none
  font-size 1em
  z-index 1
  box-shadow inset 0px 1px 0px #c4d0f9, 0px 2px 0px #AAAAAA
  background-image linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image -o-linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image -moz-linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image -webkit-linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image -ms-linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image -webkit-gradient(
  linear,
  left bottom,
  left top,
  color-stop(0, rgb(44,160,202)),
  color-stop(1, rgb(62,184,229))
  )
  &:active
    padding-bottom 7px
    padding-left 8px
    padding-right 8px
    padding-top 9px
    top 1px
    background-image linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image -o-linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image -moz-linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image -webkit-linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image -ms-linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image -webkit-gradient(
    linear,
    left bottom,
    left top,
    color-stop(0, rgb(62,184,229)),
    color-stop(1, rgb(44,160,202))
    )
  &:hover
    text-decoration underline
#content
  position relative
  margin 10px 0
  padding 0 10px
  background-color #EFEFEF
  border-color #6495ed
  border-width 1px
  border-style solid
  border-radius 15px

.align_left
  text-align left
.align_center
  text-align center
.align_right
  text-align right
.float_left
  float left
.float_right
  float right
  margin-left 1em
  margin-bottom 1em

ul, ol
  font-family 'Open Sans', sans-serif
  font-size 12pt
  color #222222
  text-align left

h2, h3
  text-align center

.scrollTop
  background url(../images/template/arrow.png) 0 0 no-repeat
  display none
  width 100px
  height 100px
  position fixed
  top 5%
  left 0

.table
  display table
  div
    display table-row
    div
      display table-cell
      padding 0 5px

a
  color blue
.greenText
  color #009635

.importantSelect
  font-size 14pt
  border 2px #009635 solid
  border-radius 10px

.roundButton
  position relative
  border 1px solid #186f8f
  border-radius 15px
  padding 8px 20px
  color #ffffff
  outline none
  box-shadow inset 0px 1px 0px #c4d0f9, 0px 2px 0px #AAAAAA
  background-color #3bb3e0
  background-image -o-linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image -moz-linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image -webkit-linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image -ms-linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  background-image linear-gradient(bottom, rgb(44,160,202) 0%, rgb(62,184,229) 100%)
  &:active
    padding 9px 20px 7px 20px
    top 1px
    background-image -o-linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image -moz-linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image -webkit-linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image -ms-linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
    background-image linear-gradient(bottom, rgb(62,184,229) 0%, rgb(44,160,202) 100%)
  &:hover
    text-decoration underline
    cursor pointer
.greenButton
  background-color #009635
  background-image -o-linear-gradient(bottom, #009635 0%, #11DA00 100%)
  background-image -moz-linear-gradient(bottom, #009635 0%, #11DA00 100%)
  background-image -webkit-linear-gradient(bottom, #009635 0%, #11DA00 100%)
  background-image -ms-linear-gradient(bottom, #009635 0%, #11DA00 100%)
  background-image linear-gradient(bottom, #009635 0%, #11DA00 100%)
  &:active
    background-color #11DA00
    background-image -o-linear-gradient(bottom, #11DA00 0%, #009635 100%)
    background-image -moz-linear-gradient(bottom, #11DA00 0%, #009635 100%)
    background-image -webkit-linear-gradient(bottom, #11DA00 0%, #009635 100%)
    background-image -ms-linear-gradient(bottom, #11DA00 0%, #009635 100%)
    background-image linear-gradient(bottom, #11DA00 0%, #009635 100%)