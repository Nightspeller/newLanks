h2, h3 {
  text-align: center;
}
p {
  font-family: 'Open Sans', sans-serif;
  font-size: 12pt;
  color: #222222;
  text-align: justify;
}
#images {
  text-align: center;
}
#images img {
  margin: 0 2em;
}
#images img:hover {
  cursor:pointer;
}
article section{
  display: none;
  border: 1px #6495ed solid;
  border-radius: 5px;
  margin-bottom: 10px;
  padding: 0 0 10px 0;
  background: #f0f8ff;
}
#section4{
  text-align: center;
}
#section4 input[type="text"]{
  font-size: 3em;
}
#section4 div{
  font-size: 1.5em;
}
#section6{
  text-align: center;
}
#section6 input[type="text"]{
  font-size: 3em;
}
#section6
  div
    font-size: 1.5em;
    text-align: left;
    padding-left: 3.1em;
  .clientWarning
    display none
    .clientWarning-overlay
      /* A dark translucent div that covers the whole screen */
      position absolute
      z-index 9999
      top 0
      left 0
      width 100%
      height 100%
      background rgba(0,0,0,0.75)
    .clientWarning-dialog
      /* A centered div above the overlay with a box shadow. */
      z-index 10000
      position absolute
      width 80%
      border 1px solid silver
      /* Center the dialog */
      top 20%
      left 20%
      transform translate(-13%, -10%)
      padding 0
      background white
      .clientWarning-content
        font-size 1em
        padding 10px
        text-align center
        color black
        p
          color black
