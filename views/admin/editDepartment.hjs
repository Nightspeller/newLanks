<html lang="en">
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <title>Material Design Bootstrap</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.0/css/font-awesome.min.css">
    <link href="vendor/css/bootstrap.min.css" rel="stylesheet">
    <link href="vendor/css/mdb.min.css" rel="stylesheet">

</head>

<body>
<header>
    <!--Navbar-->
    <nav class="navbar navbar-dark primary-color-dark">

        <!-- Collapse button-->
        <button class="navbar-toggler hidden-sm-up" type="button" data-toggle="collapse" data-target="#collapseEx">
            <i class="fa fa-bars"></i>
        </button>

        <div class="container">

            <!--Collapse content-->
            <div class="collapse navbar-toggleable-xs" id="collapseEx">
                <!--Navbar Brand-->
                <a class="navbar-brand waves-effect waves-light" href="/">ООО Ланкс</a>

                <a class="navbar-brand waves-effect waves-light" style="float: right" href="login">Выйдти</a>
            </div>
            <!--/.Collapse content-->

        </div>

    </nav>
    <!--/.Navbar-->
</header>
<main>
    <!--Main layout-->

    {{#department}}
    <div class="container">
        <br>
        <br>
        <!--Form with header-->
        <div class="card">
            <div class="card-block">

                <!--Header-->
                <div class="form-header  purple darken-4">
                    <h3>Редактирование филиала в г. {{city}}</h3>
                </div>

                <!--Body-->
                <form>
                    <div class="md-form">
                        <input type="text" id="city" class="form-control" name="city" value="{{city}}">
                        <label for="city">Город</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="address" class="form-control" name="address" value="{{address}}">
                        <label for="address">Адрес Офиса</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="email" class="form-control" name="email" value="{{email}}">
                        <label for="email">E-mail</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="skype" class="form-control" name="skype" value="{{skype}}">
                        <label for="skype">Skype</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="mainPhone" name="mainPhone" value="{{mainPhone}}">
                        <label for="mainPhone">Телефон</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="fax" class="form-control" name="fax" value="{{fax}}">
                        <label for="fax">Факс</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="additionalPhone" class="form-control" name="additionalPhone" value="{{additionalPhone}}">
                        <label for="additionalPhone">Дополнительные телефоны</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="stockAddress" class="form-control" name="stockAddress" value="{{stockAddress}}">
                        <label for="stockAddress">Адрес Склада</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="coordinates" class="form-control" name="coordinates" value="{{coordinates}}">
                        <label for="coordinates">Координаты</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="mapID" class="form-control" name="mapID" value="{{mapID}}">
                        <label for="mapID">ID яндекс-карты</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="cityEng" class="form-control" name="cityEng" value="{{cityEng}}">
                        <label for="cityEng">Английский идентификатор города</label>
                    </div>

                    <div class="md-form">
                        <input type="text" id="index" class="form-control" name="index" value="{{index}}">
                        <label for="index">Порядковый номер города</label>
                    </div>

                    <div class="md-form">
                        <img src="images/departments/{{cityEng}}.png">
                        <input id="picture" class="form-control" type="file" multiple="multiple" name="picture" placeholder="Изображение">
                    </div>

                    <div class="md-form">
                        <input type="hidden" name="_id" value="{{_id}}"/>
                    </div>

                    <div class="text-xs-center">
                        <button class="btn btn-deep-purple" type="submit">Отправить</button>
                    </div>
                </form>
                <div class="text-xs-center">
                    <button class="btn btn-danger" id="delete-department-button">Удалить филиал</button>
                </div>

            </div>
        </div>
        <!--/Form with header-->
    </div>
    {{/department}}
    <!--/.Main layout-->
</main>
<!--Footer-->
<footer class="page-footer center-on-small-only primary-color-dark">
    <!--Copyright-->
    <div class="footer-copyright">
        <div class="containter-fluid">
            © 2016 Copyright: ООО Ланкс
        </div>
    </div>
    <!--/.Copyright-->
</footer>
<!--/.Footer-->

<!-- SCRIPTS -->

<!-- JQuery -->
<script type="text/javascript" src="vendor/js/jquery-2.2.3.min.js"></script>
<script type="text/javascript" src="vendor/js/tether.min.js"></script>
<script type="text/javascript" src="vendor/js/bootstrap.min.js"></script>
<script type="text/javascript" src="vendor/js/mdb.min.js"></script>

<script type="text/javascript" src="js/admin/editDepartment.js"></script>
</body>
</html>